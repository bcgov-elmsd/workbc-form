          <input type="text" class="form-control <%= errors.lastname ? 'is-invalid' : '' %>" id="lastname"
            name="lastname" value="<%= data.lastname %>" />
          <% if (errors.lastname) { %>
          <div class="invalid-feedback"><%= errors.lastname.msg %></div>
          <% } %>
        </div>
      </div>
      <div class="form-group">
